.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", payment_source_types_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_payment_source_type_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_payment_source_type_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      PaymentSourceType
    .inner
      = form_for @payment_source_type, :url => payment_source_type_path(@payment_source_type), :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
